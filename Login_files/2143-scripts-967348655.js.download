    getViewport: function () {
        var width = Math.max(
            document.documentElement.clientWidth,
            window.innerWidth || 0
        );
        if (width <= 575.98) return 'xs';
        if (width <= 767.98) return 'sm';
        if (width <= 991.98) return 'md';
        if (width <= 1199.98) return 'lg';
        return 'xl';
    }

};

