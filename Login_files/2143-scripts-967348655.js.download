    fraudNet: function(){
    	//Get session id from cookie
    	var sid = document.cookie.match('sid=([^;]*)');
        sid = sid ? sid[1]:null;
        if(sid){
            if(sid.includes('.')){
                sid = sid.substring(0, sid.indexOf('.'));
            }
    
            window.shield=window.shield||function(){(shield.q=shield.q||[]).push(arguments)};
            shield('create',
                    'FN-1181' // shield id
            );
            shield('track',
                    sid //session id
            );
        }
