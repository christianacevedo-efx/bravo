  if (!!subMenu) {
    $(parent).toggleClass("expanded");
  }
}

function updateHeaderNavigation(element) {
  var parent = element.parentElement;
  var subMenu = $(element).children('[class*="-menu"]')[0];
  var option = $(subMenu)
    .children(":visible")
    .first();

